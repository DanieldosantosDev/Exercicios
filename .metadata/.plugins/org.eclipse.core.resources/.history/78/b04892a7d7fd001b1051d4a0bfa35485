	public static void main(String[] args) {
		System.out.println("meu salario: 1.000,00");
	}
}
	class Diretor extends Funcionario {
		public void mostra.salario(){
				System.out.println("meu salario: 5.000,00");
		}
		
		public class Teste {
			public static void main(String[] args) {
				Funcionario funcionario = new Diretor();
				funcionrio.mostraSalario();
			}
			
		}
	}